<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.hair2u.hair2uapp">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:name=".ConfigApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.Hair2uApp">
        <activity android:name="br.com.hair2u.hair2uapp.AddProfissional"></activity>
        <activity android:name="br.com.hair2u.hair2uapp.ProfissionalObs"></activity>
        <activity android:name="br.com.hair2u.hair2uapp.ProdutosActivity" />
        <activity android:name="br.com.hair2u.hair2uapp.ServicosActivity" />
        <activity
            android:name="br.com.hair2u.hair2uapp.ProfissionalActivity"
            android:theme="@style/Theme.Toolbar"
            />
        <activity
            android:name="br.com.hair2u.hair2uapp.AboutActivity"
            android:theme="@style/Theme.Toolbar" />
        <activity android:name="br.com.hair2u.hair2uapp.ConfigActivity" />
        <activity
            android:name="br.com.hair2u.hair2uapp.HomeActivity"
            android:theme="@style/Theme.Toolbar" />
        <activity android:name="br.com.hair2u.hair2uapp.DebugActivity" />
        <activity android:name="br.com.hair2u.hair2uapp.PrincipalActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <service android:name=".MyFirebaseMessagingService">
            <intent-filter>
                 <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
    </application>

</manifest>