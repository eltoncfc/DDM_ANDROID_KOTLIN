<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.Hair2u.Hair2u">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:name=".Hair2uApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.Hair2uApp">
        <activity android:name="br.com.Hair2u.Hair2u.ProdutosActivity" />
        <activity android:name="br.com.Hair2u.Hair2u.ServicosActivity" />
        <activity
            android:name="br.com.Hair2u.Hair2u.ProfissionalActivity"
            android:theme="@style/Theme.Toolbar"
            />
        <activity
            android:name="br.com.Hair2u.Hair2u.AboutActivity"
            android:theme="@style/Theme.Toolbar" />
        <activity android:name="br.com.Hair2u.Hair2u.ConfigActivity" />
        <activity
            android:name="br.com.Hair2u.Hair2u.HomeActivity"
            android:theme="@style/Theme.Toolbar" />
        <activity android:name="br.com.Hair2u.Hair2u.DebugActivity" />
        <activity android:name="br.com.Hair2u.Hair2u.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>